	sv_setref_pv ($arg, \"UV::Raw::${(my $ntt = $ntype) =~ s/_/::/g;\$ntt}\", (void *) $var);

INPUT
T_UV_RAW_REF
	if (sv_isobject ($arg) &&
	    sv_derived_from ($arg, \"UV::Raw::${(my $ntt = $ntype) =~ s/_/::/g;\$ntt}\")) {
		$var = INT2PTR ($type, SvIV ((SV *) SvRV ($arg)));
	} else {
		$var = NULL;
		croak (\"$var is not of type UV::Raw::${(my $ntt = $ntype) =~ s/_/::/g;\$ntt}\");
	}
